<extension> 
    <modules> 
        <module> 
            <id>Custom Data-lists Widget</id> 
            <description>Instantiate a custom Data-lists widget</description> 
            <auto-deploy>true</auto-deploy> 
            <customizations> 
                <customization> 
                    <targetPackageRoot>org.alfresco.components.data-lists</targetPackageRoot> 
                    <sourcePackageRoot>org.alfresco.components.data-lists.customization</sourcePackageRoot> 
                </customization> 
            </customizations> 
        </module> 
    </modules> 
    <modules>
        <module>
            <id>Customize Web Script Template for Footer</id>
            <version>1.0</version>
            <auto-deploy>true</auto-deploy>
            <customizations>
                <customization>
                    <targetPackageRoot>org.alfresco.components.footer</targetPackageRoot>
                    <sourcePackageRoot>org.alfresco.components.footer.customization</sourcePackageRoot>
                </customization>
            </customizations>
        </module>
    </modules>
</extension> 