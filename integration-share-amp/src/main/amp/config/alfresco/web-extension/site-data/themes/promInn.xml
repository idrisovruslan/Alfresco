<?xml version='1.0' encoding='UTF-8'?>
<theme>
   <title>promInn</title>
   <title-id>theme.promInn</title-id>
   <css-tokens>
      <!-- Aikau related LESS variables (requires Aikau 1.0.18) -->
      <less-variables>
// Main palette
		@primary-background-color: #6CACE4;
		@primary-font-color: #000000;
		@header-background-color: #6CACE4;
		@header-font-color: #000000;
		@alternate-background-color: #6CACE4;
		@alternate-font-color: @primary-font-color;

       
		@header-hover-background-color: #025EA1; 
		@header-hover-font-color: #ccc;
		@header-focus-background-color: #025EA1;
		@header-focus-font-color: red;
		@header-menubar-font-color: #000000;
		@header-dropdown-menu-font-color: #000000;
		
		@dashlet-background: #333333;
		@dashlet-border: 1px solid #bababa;
		@dashlet-border-radius: 0;
		@dashlet-title-background: #6CACE4;
		@dashlet-title-border-bottom: 0;
		@dashlet-title-border-radius: 0;
		@dashlet-title-color: #fff;
		@dashlet-toolbar-background: #6CACE4;
		@dashlet-toolbar-border-bottom: 1px solid #d3d3d3;
		@dashlet-body-background: #6CACE4;
		@dashlet-body-border-radius: 0;
      </less-variables>
   </css-tokens>
</theme>